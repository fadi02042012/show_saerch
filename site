    <!-- زر البحث -->
    <button class="search-button" onclick="performSearch()">
        <img src="https://upload.wikimedia.org/wikipedia/commons/6/6b/Google_Icons_-_Search.png" alt="بحث">
    </button>
</div>

<script>
    function performSearch() {
        var query = document.getElementById("searchQuery").value;
        var engineDropdown = document.getElementById("searchEnginesDropdown");
        var selectedOption = engineDropdown.options[engineDropdown.selectedIndex];
        
        if (query && selectedOption.value) {
            var baseURL = selectedOption.getAttribute("data-base");
            var suffix = selectedOption.getAttribute("data-suffix");
            var fullUrl = baseURL + encodeURIComponent(query) + suffix;
            window.location.href = fullUrl;
        } else {
            alert("يرجى إدخال كلمة البحث واختيار محرك البحث.");
        }
    }
</script>

</body>
</html>
